.index-select{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0rpx;
  background: rgba(0,0,0,.5);
  z-index: 9999;
}
.select-wrap{
  width:690rpx;
  height:866rpx;
  background: #fff;
  border-radius: 10rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -345rpx;
  margin-top: -433rpx;
}
.select-title{
  padding: 40rpx 0 30rpx;
  text-align: center;
  font-size: 36rpx;
  position: relative;
  border-bottom: 1rpx solid #eaeaea;
}
.select-close{
  position: absolute;
  right: 30rpx;
  top: 15rpx; 
  width: 26rpx;
  height: 27rpx;
}
.select-close image{
  width: 100%;
  height: 100%;
}
.select-content{
  padding: 20rpx 60rpx 0;
  font-size: 30rpx;
}
.select-one{
  display: flex;
  margin-bottom: 15rpx;
}
.select-one .name{
  width: 260rpx;
}
.select-two{
  display: flex;
}
.select-two .penghao{
  width: 260rpx;
}
.select-list{
 margin-bottom: 40rpx;
}
.select-two .next text{
  color: #69BB19;
}
.select-btn{
  text-align: center;
  padding: 30rpx;
}
.select-btn text{
  display: inline-block;
  width:332rpx;
  height:74rpx;
  line-height: 74rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: #69BB19;
  border-radius: 10rpx;
  margin: 0 auto;
}