
.no-title{
  width: 750rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #fff;
  text-align: center;
  font-size: 32rpx;
  position: relative;
}
.no-title image{
  width: 49rpx;
  height: 49rpx;
  position: absolute;
  right: 30rpx;
  top: 20rpx;
}
.no-img{
  width:236rpx;
  height:220rpx;
  margin: 210rpx auto 40rpx;
}
.no-img image{
  width: 100%;
  height: 100%;
}
.no-txt{
  width: 444rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx; 
}
.scroll-view{
  width: 100%;
  height: 90rpx;
  white-space: nowrap;
  background: #cda273;
  text-align: center;
}
.scroll-view view{
  display: inline-block;
  height: 90rpx;
  min-width: 30%;
  line-height: 90rpx;
  font-size: 32rpx; 
}
.scroll-view view.cur{
  color: #fff;
}
.yes-title{
  position: fixed;
  left: 0;
  top: 0;
  z-index: 100;
}
.yes-tianqi{
  width: 90rpx;
  height: 90rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  text-align: center;
  background: #cda273;
}
.yes-tianqi image{
  width: 49rpx;
  height: 49rpx;
  margin-top: 20rpx;
}
.yes-select{
  width: 750rpx;
  height: 100rpx;
  position: relative;
  display: flex;
  padding-top: 25rpx;
  background: #cda273;
}
.yes-select image{
  position: absolute;
  left: 0;
  top:0;
  width: 750rpx;
  height: 150rpx;
}
.yes-select view{
  position: relative;
  font-size: 28rpx;
  width:148rpx;
  height:54rpx;
  line-height: 54rpx;
  border-radius: 50rpx;
  margin-left: 50rpx;
  text-align: center;
  background: #a56734;
  color: #CDA273;
  padding-left: 15rpx;
  padding-right: 25rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.yes-select view::after{
  position: absolute;
  right: 20rpx;
  top: 25rpx;
  width: 10rpx;
  height: 10rpx;
  margin-top: -2rpx;
  border-right: 1rpx solid #CDA273;
  border-bottom: 1rpx solid #CDA273;
  -webkit-transform: rotate(45deg);
  content: '';
}
.yes-select view.cur{
  background: #f6ebdb;
}
.yes-select view.cur::after{
  position: absolute;
  right: 20rpx;
  top: 30rpx;
  width: 10rpx;
  height: 10rpx;
  margin-top: -2rpx;
  border-right: 1rpx solid #CDA273;
  border-bottom: 1rpx solid #CDA273;
  -webkit-transform: rotate(225deg);
  content: '';
}
.yes-model{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 190rpx;
  z-index: 999;
  background: rgba(0,0,0,.5);
}
.yes-content{
  width: 750rpx;
  background: #fff;
  position: absolute;
  left: 0;
  top: 0; 
  padding: 70rpx 0;
}
.yes-jiantou1,.yes-jiantou2{
  position: absolute;
  z-index: 100;
  left: 100rpx;
  top: -35rpx;
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 20rpx 20rpx 20rpx 20rpx;
  border-color: transparent transparent white transparent; 
}
.yes-jiantou2{
  left: 300rpx;
}
.yes-model-title{
  margin-left: 30rpx;
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.yes-model-list{
  display: flex;
  flex-wrap: wrap;
  font-size: 24rpx;
}
.yes-model-list view{
 padding: 10rpx 30rpx;
  text-align: center;
  background: #f8f8f8;
  border-radius: 20rpx;
  margin:0 30rpx 20rpx;
}
.yes-model-list view.cur{
  background: #ecfff2;
  color: #01C13A;
}
.yes-model-total{
  font-size: 28rpx;
  margin-left: 30rpx;
  width:184rpx;
  height:52rpx;
  line-height: 52rpx;
  text-align: center;
  background: #f8f8f8;
  border-radius: 20rpx;
  margin-bottom: 24rpx;
}
.yes-model-total.cur{
  background: #ecfff2;
  color: #01C13A;
}
.yes-list{
  margin-top: 260rpx;
  padding-bottom: 150rpx;
}
.yes-list-items{
  position: relative;
  width:612rpx;
  height:230rpx;
  margin: 0 auto 30rpx;
  background: #fff;
  padding: 30rpx;
  display: flex;
  border-radius: 10rpx;
}
.yes-item-img{
  width:192rpx;
  height:172rpx;
  margin-right: 40rpx;
}
.yes-item-img image{
  width:100%;
  height:100%;
  background: #000
}
.yes-item-title{
  margin-top: 10rpx;
  font-size: 28rpx;
  font-weight: bold;
}

.yes-item-btn text{
  display: inline-block;
  border: 1rpx solid #15BC84;
  width:119rpx;
  height:38rpx;
  line-height: 38rpx;
  text-align: center;
  font-size: 24rpx;
  color: #15BC84;
  border-radius: 10rpx;
  margin-right: 40rpx;
}
.yes-item-peng{
  font-size: 28rpx;
  color: #E7C6AE;
  margin: 20rpx auto 20rpx;
}
.yes-item-peng text{
  margin-right: 50rpx;
}
.yes-item-date{
  font-size: 24rpx;
  color: #929292;
  margin-bottom: 20rpx;
}
.yes-item-type{
  width:120rpx;
  height:40rpx;
  line-height: 40rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  position: absolute;
  right: 20rpx;
  bottom: 25rpx;
  background: #e7c6ae;
  border-radius: 20rpx;
}